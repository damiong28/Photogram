

<div class="post">
  <div class="post-head">
    <div class="name">
      Ben Walker
    </div>
  </div>
  <div class="image center-block">
    <%= image_tag @post.image.url(:medium), class:'img-responsive' %>
  </div>
  <p class="caption">
    <%= @post.caption %>
  </p>
  <div class='text-center'>
    <%= link_to "Cancel", posts_path %>
  </div> 
</div>